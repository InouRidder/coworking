<div class="container py-5">
  <div class="row py-2">
    <div class="col-12 d-flex d-flex justify-content-end">
      <%= link_to 'Add me to the waiting list!', new_registration_path, class: 'btn btn-primary' %>
    </div>
  </div>
  <div class="row">
    <div class="col-12 col-sm-6">
      <h1>
        Waiting list
      </h1>
      <ol>
        <%= render partial: 'requests/request', collection: @presenter.waiting_list %>
      </ol>
    </div>
    <div class="col-12 col-sm-6">
      <h1>
        Desks
      </h1>
      <%= render partial: 'desk', collection: @presenter.desks %>
    </div>
  </div>
</div>

